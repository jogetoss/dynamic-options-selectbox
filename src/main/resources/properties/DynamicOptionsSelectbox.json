[
    {
       "title":"@@form.dynamicoptionsselectbox.edit@@",
       "properties":[
          {
             "name":"id",
             "label":"@@form.dynamicoptionsselectbox.id@@",
             "type":"autocomplete",
             "required":"True",
             "regex_validation":"^[a-zA-Z0-9_]+$",
             "validation_message":"@@form.dynamicoptionsselectbox.invalidId@@",
             "options_callback":"FormBuilder.getAllFieldOptions"
          },
          {
             "name":"label",
             "label":"@@form.dynamicoptionsselectbox.label@@",
             "type":"textfield",
             "value":"@@org.joget.marketplace.dynamicoptionsselectbox.label@@"
          },
          {
            "label":"@@form.dynamicoptionsselectbox.populateSelectBox@@",
            "type":"header"
         },
         {
            "name":"formDefId",
            "label":"@@form.dynamicoptionsselectbox.formDefId@@",
            "description":"@@form.dynamicoptionsselectbox.formDefId.desc@@",
            "type":"selectbox",
            "options_ajax":"[CONTEXT_PATH]/web/json/console/app[APP_PATH]/forms/options",
            "required":"True"
         },
         {
            "name":"idField",
            "label":"@@form.dynamicoptionsselectbox.idField@@",
            "description":"@@form.dynamicoptionsselectbox.idField.desc@@",
            "type":"selectbox",
            "options_ajax_on_change":"formDefId",
            "options_ajax":"[CONTEXT_PATH]/web/json/console/app[APP_PATH]/form/columns/options"
         },     
         {
            "name":"displayField",
            "label":"@@form.dynamicoptionsselectbox.displayField@@",
            "description":"@@form.dynamicoptionsselectbox.displayField.desc@@",
            "type":"selectbox",
            "options_ajax_on_change":"formDefId",
            "options_ajax":"[CONTEXT_PATH]/web/json/console/app[APP_PATH]/form/columns/options",
            "required":"true"
         },
         {
            "name":"listId",
            "label":"@@form.dynamicoptionsselectbox.listId@@",
            "description":"@@form.dynamicoptionsselectbox.listId.desc@@",
            "type":"selectbox",
            "required":"True",
            "options_ajax":"[CONTEXT_PATH]/web/json/console/app[APP_PATH]/datalist/options"
         },       
         {
            "name":"requestParams",
            "label":"@@form.dynamicoptionsselectbox.requestParam@@",
            "type":"grid",
            "columns":[
               {
                  "key":"param",
                  "label":"@@form.dynamicoptionsselectbox.Parameter@@"
               },
               {
                  "key":"field",
                  "label":"@@form.dynamicoptionsselectbox.field@@",
                  "options_callback":"FormBuilder.getFieldOptions"
               },
               {
                  "key":"defaultValue",
                  "label":"@@form.dynamicoptionsselectbox.defaultValue@@"
               }
            ]
         },
         {
            "label":"@@form.dynamicoptionsselectbox.styling@@",
            "type":"header"
         },
         {
            "name":"height",
            "label":"@@form.dynamicoptionsselectbox.popupWindowHeight@@",
            "type":"textfield",
            "value":"500"
         },
         {
            "name":"width",
            "label":"@@form.dynamicoptionsselectbox.popupWindowWidth@@",
            "type":"textfield",
            "value":"900"
         },
         {
            "name":"buttonInlineStyle",
            "label":"@@form.dynamicoptionsselectbox.buttonInlineStyle@@",
            "type":"textfield"
         },
         {
            "name":"buttonLabel",
            "label":"@@form.dynamicoptionsselectbox.buttonLabel@@",
            "type":"textfield",
            "value":"Select",
            "required":"true"
         },
         {
            "name":"buttonPrimary",
            "label":"@@form.dynamicoptionsselectbox.buttonPrimary@@",
            "type":"checkbox",
            "options":[
               {
                  "value":"true",
                  "label":""
               }
            ]
         }
       ]
    },
    {
      "title":"@@form.selectbox.advancedOptions@@",
      "properties":[
         {
            "label":"@@form.selectbox.data@@",
            "type":"header"
         },
         {
            "name":"value",
            "description":"@@form.selectbox.value.desc@@",
            "label":"@@form.selectbox.value@@",
            "type":"textfield"
         },
         {
            "name":"multiple",
            "label":"@@form.selectbox.multipleSelection@@",
            "type":"checkbox",
            "value":"False",
            "options":[
               {
                  "value":"true",
                  "label":""
               }
            ]
         },
         {
            "name":"validator",
            "label":"@@form.selectbox.validator@@",
            "type":"elementselect",
            "options_ajax":"[CONTEXT_PATH]/web/property/json/getElements?classname=org.joget.apps.form.model.FormValidator",
            "url":"[CONTEXT_PATH]/web/property/json[APP_PATH]/getPropertyOptions"
         },
         {
            "label":"@@form.selectbox.ui@@",
            "type":"header"
         },
         {
            "name":"size",
            "label":"@@form.selectbox.size@@",
            "type":"textfield"
         },
         {
            "name":"readonly",
            "label":"@@form.selectbox.readonly@@",
            "type":"checkbox",
            "value":"False",
            "options":[
               {
                  "value":"true",
                  "label":""
               }
            ]
         },
         {
            "name":"readonlyLabel",
            "label":"@@form.selectbox.readonlyLabel@@",
            "type":"checkbox",
            "value":"False",
            "options":[
               {
                  "value":"true",
                  "label":""
               }
            ]
         },
         {
            "label":"@@form.selectbox.workflow@@",
            "type":"header"
         },
         {
            "name":"workflowVariable",
            "label":"@@form.selectbox.workflowVariable@@",
            "type":"autocomplete",
            "options_ajax":"[CONTEXT_PATH]/web/json/console/app[APP_PATH]/workflowVariable/options"
         }
      ]
   }
 ]